  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.2.1</version>
        <executions>
          <execution>
            <id>shade</id>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <relocations>
            <relocation>
              <pattern>de.tr7zw.changeme.nbtapi</pattern>
              <shadedPattern>com.iridium.iridiumcrates.nbtapi</shadedPattern>
            </relocation>
            <relocation>
              <pattern>com.cryptomorin.xseries</pattern>
              <shadedPattern>com.iridium.iridiumcrates.xseries</shadedPattern>
            </relocation>
            <relocation>
              <pattern>org.bstats</pattern>
              <shadedPattern>com.iridium.iridiumcrates.bstats</shadedPattern>
            </relocation>
            <relocation>
              <pattern>com.iridium.iridiumcolorapi</pattern>
              <shadedPattern>com.iridium.iridiumcrates</shadedPattern>
            </relocation>
          </relocations>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>8</source>
          <target>8</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
